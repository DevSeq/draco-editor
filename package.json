{
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "webpack-dev-server --mode development --config webpack.dev.js --open",
    "build-js": "webpack --mode production --config webpack.prod.js",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3": "^5.4.0",
    "d3-selection": "^1.3.0",
    "draco-vis": "^0.0.9",
    "history": "^4.7.2",
    "monaco-editor": "^0.13.1",
    "monaco-editor-webpack-plugin": "^1.2.1",
    "react": "^16.4.0",
    "react-animate-on-change": "^1.0.1",
    "react-dom": "^16.4.0",
    "react-json-view": "^1.17.0",
    "react-monaco-editor": "^0.17.1",
    "react-router-dom": "^4.2.2",
    "react-split-pane": "^0.1.77",
    "vega-embed": "^3.14.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/d3": "^5.0.0",
    "@types/d3-selection": "^1.3.1",
    "@types/history": "^4.6.2",
    "@types/inline-style-prefixer": "^3.0.1",
    "@types/node": "^10.3.0",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "@types/react-monaco-editor": "^0.10.1",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-split-pane": "^0.1.67",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "source-map": "^0.7.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "typescript": "^2.9.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
