{
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "webpack-dev-server --mode development --config webpack.dev.js --open",
    "build-js": "webpack --mode production --config webpack.prod.js",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "d3": "^5.7.0",
    "d3-selection": "^1.3.2",
    "draco-vis": "^0.0.12",
    "history": "^4.7.2",
    "monaco-editor": "^0.14.3",
    "monaco-editor-webpack-plugin": "^1.5.4",
    "react": "^16.5.2",
    "react-animate-on-change": "^2.1.1",
    "react-dom": "^16.5.2",
    "react-json-view": "^1.19.1",
    "react-monaco-editor": "^0.18.0",
    "react-router-dom": "^4.3.1",
    "react-split-pane": "^0.1.84",
    "vega-embed": "^3.20.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/d3": "^5.0.0",
    "@types/d3-selection": "^1.3.2",
    "@types/history": "^4.7.0",
    "@types/inline-style-prefixer": "^3.0.1",
    "@types/node": "^10.11.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "gh-pages": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "source-map": "^0.7.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "typescript": "^3.1.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  }
}
